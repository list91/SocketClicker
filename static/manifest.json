{
  "manifest_version": 2,
  "name": "parser",
  "description": "",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  
  "permissions": [
    "experimental",
    "activeTab",
    "tabs",
    "scripting",
    "alarms",
    "contentSettings",
    "contextMenus",
    "cookies",
    "history",
    "management",
    "settingsPrivate",
    "storage",
    "webNavigation",
    "*://*/*",
    "chrome://favicon/*",
    "webRequestBlocking",
    "unlimitedStorage",
    "<all_urls>",
    {
      "socket": [ "tcp-listen" ]
    }
  ]
}