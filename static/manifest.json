{
  "manifest_version": 2,
  "name": "parser",
  "description": "",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "experimental",
    "activeTab",
    "tabs",
    "scripting",
    "alarms",
    "contentSettings",
    "contextMenus",
    "cookies",
    "history",
    "management",
    "settingsPrivate",
    "storage",
    "webNavigation",
    "*://*/*",
    "chrome://favicon/*",
    "webRequestBlocking",
    "unlimitedStorage",
    "<all_urls>",
    "downloads",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}