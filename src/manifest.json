{
  "manifest_version": 2,
  "name": "Название вашего расширения",
  "version": "1.0",
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "http://127.0.0.1:5000/*"
  ]
}
